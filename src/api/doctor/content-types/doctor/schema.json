{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Address": {
      "type": "string"
    },
    "Patients": {
      "type": "string"
    },
    "Experience": {
      "type": "string"
    },
    "StartTime": {
      "type": "time",
      "required": true
    },
    "EndTime": {
      "type": "time",
      "required": true
    },
    "About": {
      "type": "blocks"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "doctors"
    },
    "Phone": {
      "type": "string"
    },
    "Image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "Premium": {
      "type": "boolean"
    },
    "hospital": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::appointment.appointment",
      "inversedBy": "hospitals"
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "doctor"
    }
  }
}
